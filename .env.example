# ===== AI 服务配置 =====
# 选择AI服务提供商: openai | deepseek | zhipu | moonshot | azure | anthropic | ollama
AI_PROVIDER=openai

# ===== OpenAI 配置 =====
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_BASE_URL=https://api.openai.com/v1
OPENAI_WHISPER_MODEL=whisper-1
OPENAI_GPT_MODEL=gpt-4o-mini

# ===== DeepSeek 配置 =====
DEEPSEEK_API_KEY=your_deepseek_api_key_here
DEEPSEEK_BASE_URL=https://api.deepseek.com/v1
DEEPSEEK_WHISPER_MODEL=whisper-1
DEEPSEEK_GPT_MODEL=deepseek-chat

# ===== 智谱AI (GLM) 配置 =====
ZHIPU_API_KEY=your_zhipu_api_key_here
ZHIPU_BASE_URL=https://open.bigmodel.cn/api/paas/v4
ZHIPU_WHISPER_MODEL=whisper-1
ZHIPU_GPT_MODEL=glm-4

# ===== Moonshot (Kimi) 配置 =====
MOONSHOT_API_KEY=your_moonshot_api_key_here
MOONSHOT_BASE_URL=https://api.moonshot.cn/v1
MOONSHOT_WHISPER_MODEL=whisper-1
MOONSHOT_GPT_MODEL=moonshot-v1-8k

# ===== Azure OpenAI 配置 =====
AZURE_API_KEY=your_azure_api_key_here
AZURE_BASE_URL=https://your-resource.openai.azure.com
AZURE_API_VERSION=2024-02-01
AZURE_WHISPER_DEPLOYMENT=whisper
AZURE_GPT_DEPLOYMENT=gpt-4

# ===== Anthropic Claude 配置 =====
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_BASE_URL=https://api.anthropic.com
ANTHROPIC_MODEL=claude-3-haiku-20240307
# 注意：Claude 不支持音频转录，会回退到其他服务

# ===== Ollama 本地配置 =====
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_WHISPER_MODEL=whisper:latest
OLLAMA_GPT_MODEL=llama3:latest

# ===== 其他配置 =====
NO_UPLOAD=0
